{
  "name": "angular-ys-service-keyboard",
  "version": "1.0.0",
  "description": "Angular service to use factory keyboard event",
  "main": "lib/angular-ys-service-keyboard.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.1",
    "bower": "^1.8.2",
    "chai": "^4.1.2",
    "karma": "^1.7.1",
    "karma-bro": "^0.11.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
